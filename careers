<script>
const locoScroll = new LocomotiveScroll({
  el: document.querySelector(".locomotive-scroll"),
  smooth: true,
  getDirection: true,
  smartphone: {
      smooth: false
  },
  tablet: {
      smooth: false
  },
  smoothMobile: 1,
  multiplier: 0.5,
  touchMultiplier: 2.0,
}); 
  
locoScroll.on('scroll', (instance) => {
    document.documentElement.setAttribute('data-direction', instance.direction)
}); 
  
</script>

